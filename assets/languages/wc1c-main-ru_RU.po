#: src/Schemas/Productscml/Admin.php:1573
#: src/Schemas/Productscml/Admin.php:1581
#: src/Schemas/Productscml/Admin.php:1742
#: src/Schemas/Productscml/Admin.php:1809
#: src/Schemas/Productscml/Admin.php:1823
msgid "Do not update"
msgstr "Не обновлять"

#: src/Schemas/Productscml/Admin.php:676 src/Schemas/Productscml/Admin.php:691
#: src/Schemas/Productscml/Admin.php:989 src/Schemas/Productscml/Admin.php:1004
#: src/Schemas/Productscml/Admin.php:1165
#: src/Schemas/Productscml/Admin.php:1180
