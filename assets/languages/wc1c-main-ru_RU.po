#: src/Schemas/Productscml/Admin.php:1766
msgid "Do not update"
msgstr "Не обновлять"

#: src/Schemas/Productscml/Admin.php:625 src/Schemas/Productscml/Admin.php:640
#: src/Schemas/Productscml/Admin.php:938 src/Schemas/Productscml/Admin.php:953
#: src/Schemas/Productscml/Admin.php:1108
